.jumbotron
  .container.text-center
    = image_tag('header_logo.png', alt: 'PROTO TYPE - SHARE THE PROTOTYPE', height: '73px')

.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li{role: 'presentation'}
          = link_to 'Popular PROTO', popular_index_path
        %li.active{role: 'presentation'}
          = link_to 'Newest PROTO', root_path
        %li{ style: 'padding: 0 0 0 690px'}
          = link_to 'View Tags', tags_path, class: 'btn btn-default', method: :get

.container.proto-list
  .row
    = render partial: 'prototype', collection: @prototypes

.text-center
  = paginate(@prototypes)
