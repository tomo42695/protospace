.col-sm-4.col-md-3.proto-conent
  .thumbnail
    = link_to image_tag(prototype.prototype_images.main.first.content), prototype_path(prototype)
    .caption
      %h3= prototype.title
      .proto-meta
        .proto-user
          = link_to "by #{prototype.user.username}", user_path(prototype.user)
        .proto-posted
          = prototype.created_at.strftime("%b %d %Y")
      %ul.proto-tag-list.list-inline
        %li
          %a{href: "#", class: "btn btn-default"} iPad
        %li
          %a{href: "#", class: "btn btn-default"} wireframe
